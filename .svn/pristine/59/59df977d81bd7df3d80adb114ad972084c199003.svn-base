package br.ufpi.estagio.cge.modelo;
/**
 * 
 * @author irvaynematheus
 *
 */
public class Movimento {
	
	private String matricula;
	private String orgao_codigo;
	private String referencia;
	private String financeiro_descricao;
	private double quantidade;
	private double valor;
	private String tipo;

	public Movimento(){}
	
	public Movimento(String matricula, String orgao_codigo, String referencia, String financeiro_descricao,
			double quantidade, double valor, String tipo) {
		super();
		this.matricula = matricula;
		this.orgao_codigo = orgao_codigo;
		this.referencia = referencia;
		this.financeiro_descricao = financeiro_descricao;
		this.quantidade = quantidade;
		this.valor = valor;
		this.tipo = tipo;
	}

	public String getMatricula() {
		return matricula;
	}

	public void setMatricula(String matricula) {
		this.matricula = matricula;
	}

	public String getOrgao_codigo() {
		return orgao_codigo;
	}

	public void setOrgao_codigo(String orgao_codigo) {
		this.orgao_codigo = orgao_codigo;
	}

	public String getReferencia() {
		return referencia;
	}

	public void setReferencia(String referencia) {
		this.referencia = referencia;
	}

	public String getFinanceiro_descricao() {
		return financeiro_descricao;
	}

	public void setFinanceiro_descricao(String financeiro_descricao) {
		this.financeiro_descricao = financeiro_descricao;
	}

	public double getQuantidade() {
		return quantidade;
	}

	public void setQuantidade(double quantidade) {
		this.quantidade = quantidade;
	}

	public double getValor() {
		return valor;
	}

	public void setValor(double d) {
		this.valor = d;
	}

	public String getTipo() {
		return tipo;
	}

	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
}
